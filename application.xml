<?xml version="1.0" encoding="utf-8" ?>
<application xmlns="http://ns.adobe.com/air/application/1.5">
  <filename>TimeCard</filename>
  <copyright>David Furber</copyright>
  <description>A time entry and todo list application for basecamp</description>
  <customUpdateUI>false</customUpdateUI>
  <name>TimeCard</name>
  <id>name.davidfurber.timecard</id>
  <version>1.18</version>
  <initialWindow>
    <content>index.html</content>
    <height>600</height>
    <width>800</width>
    <systemChrome>standard</systemChrome>
    <transparent>false</transparent>
    <visible>true</visible>
  </initialWindow>
  <icon>
    <image128x128>icons/icon_128.png</image128x128>
    <image48x48>icons/icon_48.png</image48x48>
    <image32x32>icons/icon_32.png</image32x32>
    <image16x16>icons/icon_16.png</image16x16>
  </icon>
</application>
